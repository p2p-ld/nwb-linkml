        {
          "text": "zero_or_many",
          "description": "Zero or more instances, equivalent to *"
        },
        {
          "text": "one_or_many",
          "description": "One or more instances, equivalent to +"
        },
        {
          "text": "zero_or_one",
          "description": "Zero or one instances, equivalent to ?"
        }
      ]
    },
